<?import javafx.scene.control.TextField?>

<AnchorPane xmlns:fx="http://javafx.com/fxml" fx:controller="com.atv_n.atv_n.AtvnController">
    <children>

        <Label fx:id="TempoLabel" text="0" layoutX="100" layoutY="100" style="-fx-font-size: 48px;" />


        <TextField fx:id="TempoInput" layoutX="100" layoutY="200" prefWidth="200" style="-fx-font-size: 24px;" />


        <Button fx:id="Botao" text="Iniciar" layoutX="100" layoutY="300"
                style="-fx-font-size: 24px;" onAction="#TemporizadorSimples"/>
    </children>
</AnchorPane>
